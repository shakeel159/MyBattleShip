#include <iostream>
//#include "logic.cpp"
#include "logic.h"
#include <ctime>

using namespace std;

int main()
{
    
    int width=5;
    int height=5;
    int shipCount = 4;
    
    bool **board = create_board(height, width);
    
    for (int i = 0; i < width; i++) {
        bool *row = board[i];
        for(int j=0;j<width;j++) {
            cout << row[j] << endl;
            
        }
        cout << "-----------" << endl;
    }
    

    place_ships(board, shipCount, width, height);

    cout << "Genarating new board" << endl;

    //find_ships(board, width, height)
    
    for (int i = 0; i < width; i++) {
        bool *row = board[i];
        for(int j=0;j<width;j++) {
            cout << row[j] << endl;
            
        }
        cout << "-----------" << endl;
    }
};
